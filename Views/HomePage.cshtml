@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
	ViewData["Title"] = @Model.Value("pageTitle");
	ViewData["PageDescription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}


        @await Html.PartialAsync("_BrandSection")
        
        <!-- Trusted Company START -->
        <section class="trusted-company">
            <div class="container">
                <div class="left">
                    <p class="red-text">@Model.Value("serviceFeaturesSectionTitle")</p>
                    <h1 class="title-text">@Model.Value("serviceFeaturesHeading")</h1>
                    <a class="btn-theme btn" href="@Model.Value("serviceFeaturesButtonUrl")">@Model.Value("serviceFeaturesButton") <i class="fa-solid fa-arrow-up-right"></i></a>
                </div>
                <div class="right">
                    @Html.GetBlockListHtml(Model.ServiceFeatures)
                </div>
            </div>
        </section>
        <!-- Trusted Company END-->
        
        @await Html.PartialAsync("_AboutCompany")
        @await Html.PartialAsync("_OurServices")
        @await Html.PartialAsync("_ChooseUsSection")
        @await Html.PartialAsync("_OurProjectSection")
        @await Html.PartialAsync("_TeamSection")
        @await Html.PartialAsync("_TestimonialSection")
        @await Html.PartialAsync("_ArticleSection")
        @await Html.PartialAsync("_SignupSection")
